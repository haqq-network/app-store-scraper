  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "test": "ts-node test",
    "lint": "eslint",
    "format": "prettier --write '**/*.{ts,js,tsx,jsx,cjs,mjs,css,json,md,mdx,html,yaml,yml}'",
